

<!DOCTYPE html>
<html lang="en">
<head>
    <head>
        <%- include("../partials/head.ejs") %>
    </head>
</head>
<body>
    <header>
        <%- include("../partials/navbar.ejs",{currentPage: "home"}) %>
    </header>
    <div>
        <div class="posts m-auto">
            <% players.forEach(player=> { %>
            <div class="card post my-4 text-center">
              <div class="card-header">
                Player born date
                <%= new Date(player.born_date).toLocaleDateString("en-il") %>
              </div>
              <div class="card-body">
                <h5 class="card-title">Name: <%= player.player_name %></h5>
                <a href="./team/<%= player.team_id %>"><p class="card-text">Team ID <%= player.team_id %></p></a>
                <p class="card-text">Player Rating <%= player.rating %></p>
              </div>
            </div>
            <% }) %>
        </div>
    </div>
    <footer>
        <%- include("../partials/footer.ejs") %>
    </footer>
</body>
</html>



