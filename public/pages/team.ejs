




<!DOCTYPE html>
<html lang="en">
<head>
    <head>
        <%- include("../partials/head.ejs") %>
    </head>
</head>
<body>
    <header>
        <%- include("../partials/navbar.ejs",{currentPage: "home"}) %>
    </header>
    <div>
        <div class="posts m-auto">
            <% team.forEach(team=> { %>
            <div class="card post my-4 text-center">
                <div>
                    <img class="card-image" src="<%= team.team_logo%>" alt="logo" width="50" height="50">
                    <h1><%= team.team_name%></h1>
                </div>
                <div>
                    <p class="card-body"> <%= team.team_description %></p>
                </div>
              <div class="card-body">
                <p class="card-text">League <%= team.team_country %></p>
              </div>
            </div>
            <% }) %>
        </div>
    </div>
    <footer>
        <%- include("../partials/footer.ejs") %>
    </footer>
</body>
</html>